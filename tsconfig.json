{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "esnext"
  },
  "exclude": ["src/swagger", "src/gh-swagger-ui", "src/webpack"]
}